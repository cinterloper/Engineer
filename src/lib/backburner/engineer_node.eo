class Engineer.Node(Efl.Object)
{
   eo_prefix: engineer_node;

   implements
   {
      Efl.Object.constructor;
      Engineer.Database;
   }

   events
   {
      tick;
   }

   methods
   {
      @property clock
      {
         set {}
         get {}
         values
         {
            tickcount: uint;
         }
      }

      @property isalive
      {
          set {}
          get {}
          values
          {
              isalive: bool;
          }
      }
   }

}
